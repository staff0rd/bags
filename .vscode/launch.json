    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Scraper/bin/Debug/netcoreapp3.1/coach_bags_selenium.dll",
            //"args": [ "tweet", "-p", "-c", "OutnetCoats", "--count=30"],
            //"args": [ "tweet", "-p", "-c", "FarfetchDresses", "--count=30"],
            //"args": [ "tweet", "-p", "-c", "ToryBurchBags", "--count=300"],
            //"args": [ "tweetonly", "-p", "-c", "ToryBurchBags", "-s=2020-10-01"],
            "args": [ "testmeta", "-c", "ToryBurchBags", "--id", "74650"],
            //"args": [ "test"],
            //"args": [ "backfill", "-o",],
            "cwd": "${workspaceFolder}/src",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "DOTNET_ENVIRONMENT": "Development",
                "Headless": "false",
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
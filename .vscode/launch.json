    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Scraper/bin/Debug/netcoreapp3.1/coach_bags_selenium.dll",
            "args": [ "tweet", "-p", "-c", "FwrdDresses", "--count=100"],
            //"args": [ "tweet", "-p", "-c", "MichaelKorsBags", "--count=400"],
            //"args": [ "tweetonly", "-p", "-c", "MichaelKorsBags", "--id=30H9LUSL2T"],
            // "args": [ "twitterimages", "-c", "PradaBags", "-n", "2020-11-01T03:27", 
            //     "--source", "local/LegitBags/images/2020/11/01/0327-1.jpg",
            //     "--source", "local/LegitBags/images/2020/11/01/0327-2.jpg",
            //     "--source", "local/LegitBags/images/2020/11/01/0327-3.jpg",
            //     "--source", "local/LegitBags/images/2020/11/01/0327-4.jpg",
            //     "--source", "local/LegitBags/images/2020/11/01/0327-5.jpg"]
            //"args": [ "tweetonly", "-p", "-c", "ToryBurchBags", "-s=2020-10-01"],
            //"args": [ "testmeta", "-c", "RebeccaMinkoffBags", "--id", "HF16ICKC17-champagne"],
            //"args": [ "test"],
            //"args": [ "backfill", "-o",],
            //"args": [ "export", "-a", "-e", "FwrdDresses"],
            "cwd": "${workspaceFolder}/src",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "DOTNET_ENVIRONMENT": "Development",
                "Headless": "false",
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}